{
  "id": "sample-configuration",
  "dashboards": [
    "default"
  ],
  "timewindows": [
    {
      "title": "4 Hours",
      "from": "-4hours",
      "to": "now",
      "transform": {
        "graphite": "summarize(%s, \"5min\")"
      }
    },
    {
      "title": "2 Days",
      "from": "-2days",
      "to": "now",
      "transform": {
        "graphite": "summarize(%s, \"30min\")"
      }
    },
    {
      "title": "1 Week",
      "from": "-1week",
      "to": "now",
      "transform": {
        "graphite": "summarize(%s, \"1hour\")"
      }
    },
    {
      "title": "1 Month",
      "from": "-30days",
      "to": "now",
      "transform": {
        "graphite": "summarize(%s, \"1day\")"
      }
    },
    {
      "title": "2 Months",
      "from": "-60days",
      "to": "now",
      "transform": {
        "graphite": "summarize(%s, \"1day\")"
      }
    }
  ]
}
